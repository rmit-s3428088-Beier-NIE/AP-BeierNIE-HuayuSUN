package Game;

public class Game {
    private int ID; 
    private int gameID;
    private int gameTime;
    private int point;
	 
	public Game(int ID, int gameID, int gameTime,int point){
    	 this.gameID=gameID;
    	 this.gameTime=gameTime;
    	 this.point=point;
	}
	
    public int getGameID() {
    			return gameID;
    }

    public void setGameID(int gameID) {
    			this.gameID=gameID;
    }

	public  int getGameTime() {
		return gameTime;
	}

	public void setGameTime(int gameTime) {
		this.gameTime = gameTime;
	}

	public int getPoint() {
		return point;
	}

	public void setPoint(int point) {
		this.point = point;
	}

	public int getID() {
		return ID;
	}

	public void setID(int iD) {
		ID = iD;
	}
}
