	     Swim swim = new Swim;
	     Swim.getGameTime();
		
	     //游泳比赛时间为10-20秒
    	Random swimTime = new Random();
        getGameTime() = swim.nextInt(11)+10; 
        
    }
	
	// 每个游戏有4-8个运动员， 随机个数 randNum
    Random athleteNum = new Random();
    int swimmerNumber = athleteNum.nextInt(5)+4;
    { 
    for( int i=0;i<swimmerNumber;i++){
    	Swimmer[] SwimmerList=new Swimmer[swimmerNumber];
